<h1>Task Saver</h1>

<h3>Save the tasks you need to get to later</h3>

<ul>
  {{#each burgers}}
  	<li>
  	   <p>ID: {{this.id}}</p>
       <p>burger: {{this.burger_name}}</p>
	    <p>eaten: {{this.devoured}}</p>

  	</li>
  {{/each}}
</ul>

<form action="/" method="POST">
	<textarea type="text" name="burger_name"></textarea>

	<button type="submit">Submit</button>
</form>
